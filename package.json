{
  "name": "rockstat-website",
  "bin": {
    "rstgen": "./bin/rstgen.js"
  },
  "scripts": {
    "analyze": "cross-env ANALYZE=1 next build && cat .next/stats.txt",
    "start:dev": "node server.js",
    "start:serve": "serve",
    "start": "NODE_ENV=production node server.js",
    "clean": "rimraf public && rimraf .next",
    "prepare": "yarn clean && rstgen docs",
    "build:dev": "yarn prepare && next build",
    "build": "yarn prepare && NODE_ENV=production next build",
    "build:export": "yarn prepare && next build && next export -o ./public",
    "export:zeit": "next export -o /public"
  },
