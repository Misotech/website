{
  "name": "rockstat",
  "alias": [
    "rock.st"
  ],
  "env": {
    "HOST": "0.0.0.0",
    "PORT": "8080"
  },
  "type": "static",
  "static": {
    "public": "public",
    "redirects": [
      { "source": "/", "destination": "/ru", "type": 303 }
    ],
    "directoryListing": false,
    "trailingSlash": true
  }
}
