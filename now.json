{
  "name": "rockstat",
  "alias": [
    "rock.st"
  ],
  "env": {
    "HOST": "0.0.0.0",
    "PORT": "8080"
  },
  "type": "static",
  "static": {
    "public": "public",
    "redirects": [
      { "source": "/", "destination": "/ru/", "type": 301 },
      { "source": "/docs/", "destination": "/docs/intro/about/", "type": 301 }
    ],
    "directoryListing": false,
    "trailingSlash": true
  }
}
